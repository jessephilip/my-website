{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug: My Website",
      "remoteRoot": "/usr/src",
      "localRoot": "${workspaceFolder}",
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "address": "0.0.0.0",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Debug Nest Framework",
      "sourceMaps": true,
      "cwd": "${workspaceRoot}/server",
      "port": 9229,
      "restart": true,
      "protocol": "inspector",
      "outFiles": ["$workspaceRoot/server/dist/server"]
    }
  ]
}
